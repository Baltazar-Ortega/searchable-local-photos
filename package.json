{
  "name": "searchable-local-photos",
  "version": "1.0.0",
  "description": "Search through your photos using text",
  "main": "app/index.js",
  "scripts": {
    "start": "electron-forge start",
    "sass:build": "node-sass app/scss -o app/css",
    "sass:watch": "node-sass -w app/render/scss -o app/render/css",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "baltazarortega",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^11.1.1",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "drag-drop": "^6.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "file-type": "^16.5.3",
    "fs-extra": "^10.0.0",
    "jimp": "^0.16.1",
    "onnxruntime-node": "^1.9.0",
    "open": "^8.4.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "searchable_local_photos"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
